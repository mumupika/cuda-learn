CompileFlags:
    Remove:
        - "-forward-unknown-to-host-compiler"
        - "--generate-code=arch=compute_89,code=[compute_89,sm_89]"
        - "--extended-lambda"
        - "-G"
    Add:
        - "-std=c++17"
        - "--cuda-gpu-arch=sm_89"
        - "-I/usr/include/opencv4"
        - "-I/usr/local/cuda/include"
    Compiler: clang++